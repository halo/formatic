<%= form_for(House.new, url: '#') do |f| %>
  <%= render Formatic::Toggle.new(f:, attribute_name: :floors, class: :inside, wrapper_class: :outside) %>
  <%= render Formatic::Toggle.new(f:, attribute_name: :longthing) %>
